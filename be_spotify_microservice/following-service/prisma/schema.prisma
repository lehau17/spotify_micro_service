generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Following {
  id                Int      @id @default(autoincrement())
  user_id           Int?
  following_user_id Int?
  status            Status   @default(Enable)
  created_at        DateTime @default(now()) @db.Timestamp(6)

  @@index([user_id])
  @@index([following_user_id])
}

enum Status {
  Enable
  Disable
}
